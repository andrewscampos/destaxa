<channel-adaptor name="my-channel-adaptor" class="org.jpos.q2.iso.ChannelAdaptor" logger="Q2">
    <channel class="org.jpos.iso.channel.ASCIIChannel" logger="Q2" packager="org.jpos.iso.packager.GenericPackager">
        <property name="packager-config" value="cfg/tef.xml"/>
        <property name="host" value="localhost"/>
        <property name="port" value="5000"/>
        <property name="timeout" value="30000"/>
    </channel>

    <in>my-channel-adaptor-in</in>
    <out>my-channel-adaptor-out</out>
</channel-adaptor>
